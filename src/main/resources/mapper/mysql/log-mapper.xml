<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.hao.learn.person.dao.LogDao">
    <insert id="insertLog">
        INSERT INTO `LOG_INFO`
        VALUES(#{logInfo.id}, #{logInfo.key}, #{logInfo.success}, #{logInfo.primaryParameter}, #{logInfo.remark})
    </insert>
</mapper>